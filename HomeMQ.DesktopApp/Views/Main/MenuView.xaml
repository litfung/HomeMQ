<views:MvxWpfView x:Class="HomeMQ.DesktopApp.Views.MenuView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HomeMQ.DesktopApp.Views"
        xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
        mc:Ignorable="d" d:Height="450" d:Width="200">
    <StackPanel DockPanel.Dock="Left" Background="{StaticResource MenuBackgroundBrush}">
        <Button Content="Overview Controls" Command="{Binding ShowFirstPageCommand}"
                    Style="{StaticResource NavigationButton}"
                    />
        <Button Content="Not there" 
                    Style="{StaticResource NavigationButton}"
                    Command="{Binding ShowSecondPageCommand}"/>
    </StackPanel>
</views:MvxWpfView>
